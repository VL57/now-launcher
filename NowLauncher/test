echo COMPILING
echo
ant debug

echo
echo UNISTALLING
~marco/adt-bundle-linux-x86/sdk/platform-tools/adb uninstall com.nowlauncher.nowlauncher

echo
echo INSTALLING
~marco/adt-bundle-linux-x86/sdk/platform-tools/adb install bin/NowLauncher-debug.apk

echo
echo REMOVING PREVIOUS LOGCAT
rm a

echo
echo LOGCATTING

~marco/adt-bundle-linux-x86/sdk/platform-tools/adb logcat > a
